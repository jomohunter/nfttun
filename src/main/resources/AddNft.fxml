<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.badbud.nfttun.controllers.AddNft">
   <children>
      <TextField fx:id="NFTNameTF" layoutX="96.0" layoutY="67.0" promptText="name" />
      <TextField fx:id="NFTPriceTF" layoutX="96.0" layoutY="115.0" promptText="price" />
      <TextField fx:id="NFTStatusTF" layoutX="96.0" layoutY="156.0" promptText="status" />
      <TextField fx:id="NFTImageTF" layoutX="96.0" layoutY="200.0" promptText="image" />
      <Button layoutX="331.0" layoutY="181.0" mnemonicParsing="false" onAction="#newNFT" text="ADD NFT" />
      <Button layoutX="495.0" layoutY="14.0" mnemonicParsing="false" onAction="#ShowNftList" text="Show NFT List" />
   </children>
</AnchorPane>
